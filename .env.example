AICEBERG_BASE_URL="https://api.test1.aiceberg.ai"
AICEBERG_API_URL="https://api.test1.aiceberg.ai/eap/v1/event"
AICEBERG_API_KEY="<YOUR_AICEBERG_API_KEY>"
AICEBERG_PROFILE_ID="<YOUR_AICEBERG_PROFILE_ID>"
AICEBERG_USE_CASE_ID="<YOUR_AICEBERG_USE_CASE_ID>"
AICEBERG_USER_ID="cowork_agent_yourname"
AICEBERG_MODE="enforce"
AICEBERG_ENABLED="true"
AICEBERG_FAIL_OPEN="true"
AICEBERG_TIMEOUT="15"

# Telemetry control: Skip sending lifecycle/telemetry events to API (default: true)
# When true, events like SessionEnd, Setup, Notification, etc. are logged locally only
# Set to false if you want ALL events sent to Aiceberg dashboard
AICEBERG_SKIP_TELEMETRY_API_SEND="true"

# LLM transcript events: log locally only (default: true for initial testing)
# When true, agt_llm events from transcript reconstruction are logged locally only
# Set to false to enable live sending of LLM events to Aiceberg API
AICEBERG_LLM_TRANSCRIPT_LOCAL_ONLY="true"

AICEBERG_TINY_DEBUG_MODE="false"
AICEBERG_DEBUG_TRACE="false"
AICEBERG_DEBUG_TRACE_PATH="/tmp/aiceberg-claude-hooks/tiny-debug-trace.jsonl"
